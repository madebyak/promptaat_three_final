{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["fra1"],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://promptaat.com"
  },
  "crons": [
    {
      "path": "/api/cron/reconcile-subscriptions",
      "schedule": "0 3 * * *"
    }
  ]
}
